<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MenuActivity">

    <android.support.design.widget.FloatingActionButton
            android:id="@+id/fab_item"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            app:srcCompat="@drawable/ic_playlist_add_black_24dp"
            android:layout_margin="16dp"
            app:backgroundTint="@color/colorPrimary"/>
    <TextView
            android:text="検索機能は開発中です"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_alignParentStart="true" android:layout_marginStart="6dp"
            android:layout_alignParentTop="true" android:id="@+id/textView2" android:layout_marginTop="6dp"/>

</RelativeLayout>